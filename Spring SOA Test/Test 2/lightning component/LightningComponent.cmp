<aura:component controller="AccountController">
    <aura:attribute name="accounts" type="Account[]" />
    <aura:handler name="init" value="{!this}" action="{!c.doInit}" />
    <aura:iteration items="{!v.accounts}" var="account">
        <p>{!account.Name}</p>
    </aura:iteration>
</aura:component>
